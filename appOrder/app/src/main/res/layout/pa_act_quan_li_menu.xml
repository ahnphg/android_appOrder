<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/r4z1j6cbuivl"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@drawable/cr20bffffff">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ri8ns1q4saoa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="34dp"
                android:layout_marginStart="24dp"
                android:orientation="horizontal">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/rzfm4gt32b6s"
                    app:shapeAppearanceOverlay="@style/cr20"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginEnd="29dp"
                    android:src="@drawable/icon_back"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/rw6lk7cnj89o"
                    android:textColor="#31343D"
                    android:textSize="20sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Quản lý Thực Đơn"
                    />
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerViewMenu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingBottom="313dp"
                android:layout_marginHorizontal="20dp"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/rq6qv09bed6g"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_marginBottom="20dp"
                android:orientation="vertical">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/imgThemMonAn"
                    app:shapeAppearanceOverlay="@style/cr20"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:background="#000000"
                    android:layout_marginEnd="16dp"
                    android:src="@drawable/ic_add"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/rsveldwxbdfo_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <ImageView
                    android:id="@+id/rsveldwxbdfo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
                <LinearLayout
                    android:paddingVertical="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:background="@drawable/border"                    >
                    <LinearLayout
                        android:id="@+id/r8pgxh9hw4vv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="1dp"
                        android:layout_marginHorizontal="45dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/rr7x3a5tzrvo"
                            android:layout_width="34dp"
                            android:layout_height="34dp"
                            android:layout_margin="5dp"
                            android:src="@drawable/ic_home"
                            android:scaleType="fitXY"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <ImageView
                            android:id="@+id/roa4vumrb39p"
                            android:layout_width="34dp"
                            android:layout_height="34dp"
                            android:src="@drawable/ic_order"
                            android:scaleType="fitXY"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <ImageView
                            android:id="@+id/rxq3q6wp9vnd"
                            android:layout_width="34dp"
                            android:layout_height="34dp"
                            android:src="@drawable/ic_noti"
                            android:scaleType="fitXY"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <ImageView
                            android:id="@+id/r03uk2u6zis7z"
                            android:layout_width="34dp"
                            android:layout_height="34dp"
                            android:src="@drawable/ic_pro5"
                            android:scaleType="fitXY"
                            />
                    </LinearLayout>

                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>